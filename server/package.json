{
  "name": "fuku_uchi_erp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=$npm_package_config_dev_mode && webpack --mode development && node ./dist/",
    "watch": "cross-env NODE_ENV=$npm_package_config_dev_mode && webpack --mode development --watch",
    "build": "cross-env NODE_ENV=$npm_package_config_dev_mode && webpack --mode production",
    "server": "cross-env NODE_ENV=$npm_package_config_dev_mode && ts-node-dev src/main/main.ts",
    "test": "cross-env NODE_ENV=$npm_package_config_test_mode &&  jest --silent"
  },
  "config": {
    "dev": {
      "mode": "development"
    },
    "test": {
      "mode": "test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/satoRoger/timecard.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/satoRoger/timecard/issues"
  },
  "homepage": "https://github.com/satoRoger/timecard#readme",
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/cors": "2.8.6",
    "@types/express": "^4.16.1",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.5",
    "cross-env": "^7.0.2",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "parcel-bundler": "^1.12.4",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/**/*.test.ts"
    ]
  },
  "dependencies": {
    "@types/config": "0.0.36",
    "@types/log4js": "^2.3.5",
    "@types/luxon": "^1.25.0",
    "axios": "^0.20.0",
    "config": "^3.3.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase-admin": "^9.4.1",
    "firebase-functions": "^3.12.0",
    "inversify": "4.13.x",
    "log4js": "^6.3.0",
    "luxon": "^1.25.0",
    "reflect-metadata": "^0.1.13"
  }
}
