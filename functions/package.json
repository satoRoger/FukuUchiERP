{
  "name": "fuku_uchi_erp",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/src/main/main.js",
  "scripts": {
    "lint": "eslint \"src/**/*\"",
    "build": "tsc",
    "start": "cross-env NODE_ENV=$npm_package_config_dev_mode && npm run build && firebase emulators:start  --import=./firestore-data --only functions,firestore",
    "deploy": "cross-env NODE_ENV=$npm_package_config_dev_mode && npm run build && firebase deploy --only functions",
    "test": "cross-env NODE_ENV=$npm_package_config_test_mode &&  jest --silent",
    "clean": "rm -rf node_modules/ && npm install",
    "downloadFromGS": "rm -rf ./firestore-data/ && gsutil cp -r gs://fukuuchierp.appspot.com/%date%/firestore-data/ .",
    "exportFirestore": "gcloud firestore export gs://fukuuchierp.appspot.com/%date%/firestore-data"
  },
  "engines": {
    "node": "12"
  },
  "config": {
    "dev": {
      "mode": "development"
    },
    "test": {
      "mode": "test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/satoRoger/timecard.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/satoRoger/timecard/issues"
  },
  "homepage": "https://github.com/satoRoger/timecard#readme",
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/cors": "2.8.6",
    "@types/express": "^4.16.1",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.5",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^0.2.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "parcel-bundler": "^1.12.4",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.4",
    "ts-node-dev": "^1.0.0-pre.63",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/**/*.test.ts"
    ]
  },
  "dependencies": {
    "@types/config": "0.0.36",
    "@types/log4js": "^2.3.5",
    "@types/luxon": "^1.25.0",
    "axios": "^0.20.0",
    "config": "^3.3.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "express-validator": "^6.9.2",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.0",
    "inversify": "4.13.x",
    "log4js": "^6.3.0",
    "luxon": "^1.25.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.0.0",
    "typeorm": "^0.2.29",
    "typescript": "^4.0.3",
    "yarn": "^1.22.10"
  }
}
