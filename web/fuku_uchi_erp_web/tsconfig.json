{
  "compilerOptions": {
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "target": "es5",
    "lib": ["es6","dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
    "rootDir": "src",
    "outDir": "build"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/components/index.js",
    "stories/helloWorld.stories.js", "src/stories/sidebar.stories.js", "src/components/sidebar.js", "stories/sidebar.stories.js"
  ],
  "exclude": ["node_modules"]
}
