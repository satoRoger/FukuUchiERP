{
  "name": "fuku_uchi_erp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=$npm_package_config_dev_mode && webpack --mode development && node ./dist/",
    "watch": "cross-env NODE_ENV=$npm_package_config_dev_mode && webpack --mode development --watch",
    "build": "cross-env NODE_ENV=$npm_package_config_dev_mode && webpack --mode production",
    "server": "cross-env NODE_ENV=$npm_package_config_test_mode && webpack-dev-server --open",
    "test": "cross-env NODE_ENV=$npm_package_config_test_mode &&  jest -o",
    "test:w": "cross-env NODE_ENV=$npm_package_config_test_mode && jest --watchAll"
  },
  "config": {
    "dev": {
      "mode": "development"
    },
    "test": {
      "mode": "test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/satoRoger/timecard.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/satoRoger/timecard/issues"
  },
  "homepage": "https://github.com/satoRoger/timecard#readme",
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.5",
    "cross-env": "^7.0.2",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/**/*.test.ts"
    ]
  },
  "dependencies": {
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13"
  }
}
